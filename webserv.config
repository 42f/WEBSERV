server {
	autoindex	on;
	listen 0.0.0.0:8000;
	# this is a comment

	server_name		webserv;

	index	hello.html;
	root /var/www/html;

	cgi .php /usr/bin/php-cgi;
	#cgi .bla ./cgi/ubuntu_cgi_tester;

	error_page 500 502 503 504 /custom_50x.html;
#	error_page 500 /custom_50x.html;

	location / {
		# this is a comment
		index vector.conf;

		autoindex off;
	}

	location /factory {
		index vector.conf;
		return 302;
	}

	location /waffles {
		return 301 $scheme://127.0.0.1/factory$request_uri$request_query;
	}

	autoindex	off;
	client_max_body_size 0;

	location /storage {
		root /var/www;

		autoindex on;
		client_max_body_size 10M;
		methods GET POST;
		index index.html;

	}

	root /etc/;

	location /up {
		root /var/www/html/storage;
		autoindex off;

		upload on ;
		methods GET POST;
	}

}

