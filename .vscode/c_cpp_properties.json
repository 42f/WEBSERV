{
  "configurations": [
    {
      "name": "macos-clang-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/src/**",
        "${workspaceFolder}/src/HTTP/**",
        "${workspaceFolder}/src/config/**",
        "${workspaceFolder}/src/HTTP/Headers/**",
        "${workspaceFolder}/src/HTTP/Request/**",
        "${workspaceFolder}/src/HTTP/Response/**",
        "${workspaceFolder}/src/HTTP/Response/ResponseUtils/**",
        "${workspaceFolder}/src/parser/**",
        "${workspaceFolder}/src/parsing/**",
        "${workspaceFolder}/src/Network/**",
        "${workspaceFolder}/src/CGI/**",
        "${workspaceFolder}/src/utils/**"
      ],
      "compilerPath": "/usr/bin/clang",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "macos-clang-x64",
      "compilerArgs": [
        "-Wall",
        "-Wextra",
        "-Wpedantic"
      ]
    }
  ],
  "version": 4
}
